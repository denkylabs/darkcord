{
  "name": "darkcord",
  "description": "A NodeJS and Deno Discord library to interact with API",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/denkylabs/darkcord.git"
  },
  "bugs": {
    "url": "https://github.com/denkylabs/darkcord/issues"
  },
  "scripts": {
    "dnt": "cd dnt && deno run -A ./dnt.ts",
    "dnt:darkcord": "cd packages/darkcord && deno run -A .dnt.ts",
    "dnt:rest": "cd packages/rest && deno run -A .dnt.ts",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix"
  },
  "author": "Liy <liy@denkylabs.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "discord-api-types": "0.37.2",
    "ws": "8.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-lerna-scopes": "17.0.2",
    "@types/node": "18.6.4",
    "@types/pako": "2.0.0",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "eslint": "8.22.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "husky": "8.0.1",
    "lerna": "5.3.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16.9.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
